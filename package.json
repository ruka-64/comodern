{
  "name": "comodern",
  "version": "0.1.3",
  "description": "A Modern console wrapper",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "minify": "cp ./dist/index.js ./dist/before.js && rm ./dist/index.js && npx uglify-js -c --mangle -o ./dist/index.js ./dist/before.js && rm ./dist/before.js",
