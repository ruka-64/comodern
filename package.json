  },
  "type": "module",
  "keywords": [
    "console",
    "logger"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "https://github.com/ruka-64/comodern"
  },
  "author": "ruka-64",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "typescript": "^5.8.3",
    "uglify-js": "^3.19.3"
  },
  "dependencies": {
    "picocolors": "^1.1.1"
  }
}
